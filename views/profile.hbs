<div class="Header">
  <a class="headLeft" href="/logout"> < Logout</a>
  <h1 class="Title">{{user.firstName}}'s Scan History</h1>
  <a class="headRight" id="headAddNew" href="/scan">+</a>
</div>

<div id="itemList-container">
  <a href="/scan">
    <div class="itemElement">
      <img src="/images/plus.png" alt="">
      <p>Scan New Object</p>
    </div>
  </a>
{{#each user.scan}}
  <div>
    <div class="itemElement">
      <a href="/scanplay/{{this.id}}">
        
          <img src="/images/scan/{{this.name}}" alt="">
          <p>{{this.labels.0.description}}</p>
        
      </a>
      <a id="delete" href="/scanplay/delete/{{this.id}}">delete this</a>
    </div>
  </div>
{{/each}}
</div>
